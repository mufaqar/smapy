{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev -p 3002",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "confirm": "yarn lint --fix && tsc && env-cmd -f .env next build",
    "set-db-permissions": "env-cmd -f .env zx ./prisma/set-db-permissions.mjs",
    "run-admin-command": "node -r ts-node/register/transpile-only run-admin-command.ts",
    "run-admin-command-dbg": "env-cmd -f .env node --inspect-brk -r ts-node/register/transpile-only run-admin-command.ts",
    "get-env": "netlify env:list --json > .env-staging.json && zx ./scripts/netlify-env-json-to-dot-env.mjs --envName=staging",
    "set-env": "netlify env:import .env-staging",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@happykit/flags": "^3.0.0",
    "@hookform/resolvers": "^2.9.11",
    "@prisma/client": "^4.9.0",
    "@react-hook/debounce": "^4.0.0",
    "@supabase/auth-helpers-nextjs": "^0.5.4",
    "@supabase/auth-helpers-react": "^0.3.1",
    "@supabase/supabase-js": "^2.7.1",
    "@tanstack/react-query": "^4.20.0",
    "@tanstack/react-table": "^8.7.8",
    "@trpc/client": "^10.9.0",
    "@trpc/next": "^10.9.0",
    "@trpc/react-query": "^10.9.0",
    "@trpc/server": "^10.9.0",
    "chakra-ui": "^0.3.9",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^9.0.2",
    "json-colorz": "^0.2.7",
    "libphonenumber-js": "^1.10.14",
    "next": "13.1.6",
    "p-map": "^5.5.0",
    "rambda": "^7.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.1",
    "superjson": "1.9.1",
    "trpc-playground": "^1.0.1",
    "ts-node": "^10.9.1",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.7",
    "env-cmd": "^10.1.0",
    "eslint": "^8.30.0",
    "eslint-config-next": "13.1.6",
    "postcss": "^8.4.14",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.9.0",
    "storybook-addon-designs": "^6.3.1",
    "tailwindcss": "^3.2.0",
    "typescript": "^4.9.4",
    "zod-prisma": "^0.5.4"
  },
  "ct3aMetadata": {
    "initVersion": "7.5.1"
  },
