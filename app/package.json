{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && prisma migrate deploy && zx ./prisma/set-db-permissions.mjs && next build",
    "dev": "next dev -p 3002",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "start": "next start",
    "confirm": "yarn lint --fix && tsc && env-cmd -f .env next build && yarn prettier --write .",
    "set-db-permissions": "env-cmd -f .env zx ./prisma/set-db-permissions.mjs",
    "run-admin-command": "node -r ts-node/register/transpile-only run-admin-command.ts",
    "run-admin-command-dbg": "env-cmd -f .env node --inspect-brk -r ts-node/register/transpile-only run-admin-command.ts",
    "get-env": "netlify env:list --json > .env-staging.json && zx ./scripts/netlify-env-json-to-dot-env.mjs --envName=staging",
    "set-env": "netlify env:import .env-staging",
    "i18next-scan-translation": "../node_modules/.bin/i18next && (cd ../scripts && node ./generate-zz-translation-files/generate-zz-translation-files.mjs) && echo OPEN http://localhost:3002/system/extract-runtime-translation"
  },
